local previousHealth = 0
local OHKOTestOriginalText = nil

function getCurrentHealth()
    local addr = gtasa:getInteger("0x77CD98")
    if addr == nil then
       return 0
    end
    local result = readFloat(addr+1344)
    if result == nil then
        return 0
    end
    return result
end

function checkCurrentHealth()
    local health = getCurrentHealth()
    if OHKOTestOriginalText == nil then
        OHKOTestOriginalText = GTASAForm.OHKOTestCheckbox.Caption
    end
    if health &lt; previousHealth then
        beep()
    end
    previousHealth = health
    GTASAForm.OHKOTestCheckbox.Caption = string.format("%s [Current: %.1f]",
        OHKOTestOriginalText, health)
end

function getCurrentTimeInMs2()
    local result = gtasa:getInteger("0x6499A0")
    if result == nil then
        return -1
    end
    return result
end

local previousCurrentTimeInMs2 = 0
local currentTimeInMs2TestOriginalText = nil

function checkCurrentTimeInMs2()
    local current = getCurrentTimeInMs2()
    if current == -1 then
        return
    end
    if current ~= previousCurrentTimeInMs2 then
        beep()
        local memo = GTASAForm.TextOutput3Memo
        local x,y = getPlayerXYZ()
        if x ~= nil then
            local coords = string.format("%.2f,%.2f", x, y)
            memo.Lines.Text = memo.Lines.Text.."\n"..coords
        end
    end
    if currentTimeInMs2TestOriginalText == nil then
        currentTimeInMs2TestOriginalText = GTASAForm.EnableTimeSound.Caption
    end
    previousCurrentTimeInMs2 = current
    GTASAForm.EnableTimeSound.Caption = string.format("%s [Current: %d]",
        currentTimeInMs2TestOriginalText, current)
end

function getPlayerXYZ()
    local addr = gtasa:getInteger("0x77CD98")
    if addr == nil then
       return nil
    end
    addr = readInteger(addr+20)
    if addr == nil then
        return nil
    end
    local x = readFloat(addr+48)
    local y = readFloat(addr+52)
    local z = readFloat(addr+56)
    return x,y,z
end

