function timer()
	--update(progressValues, "E:/v.txt", 0.001)
	--update(statValues, "E:/v2.txt", 1)
    local formOutput = ""
    if enabled("WriteDupingValuesCheckbox") then
        local file = value("DupingValuesFile")
	    local result = update(phoneValues, file, 1)
        formOutput = formOutput..result.."\n"
    end
    if enabled("WriteOtherValuesCheckbox") then
	    local result = update(misc, "E:/misc.txt", 1)
        formOutput = formOutput..result.."\n"
    end
    if enabled("WriteThreadsCheckbox") then
       local result = writeThreads(nil)
       formOutput = formOutput..result.."\n"
    end
    GTASAForm["TextOutputMemo"].Lines.Text = formOutput
end

