local gameVersionOffset = 0

function adjustAddress(addr)
         if type(addr) == "string" then
            addr = tonumber(addr)
         end
         if addr == nil then
            return nil
         end
         return addr+gameVersionOffset
end

function getInteger(addr)
         return readInteger(adjustAddress(addr))
end

function getByte(addr)
         return readBytes(adjustAddress(addr), 1, false)
end

function getFloat(addr)
         return readFloat(adjustAddress(addr))
end

