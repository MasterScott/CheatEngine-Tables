function autoAttach()
    if not enabled("AutoAttachCheckbox") then
       return
    end
    tryAttach("gta_sa.exe")
    tryAttach("gta-sa.exe")
end

function tryAttach(process, force)
    --debug("Check "..process)
    local processId = getProcessIDFromProcessName(process)
    if (processId ~= getOpenedProcessID() or force) and processId ~= nil then
       openProcess(process)
       currentProcess = process
       debug("Opened process "..process)
       detectVersion()
    end
end

local autoAttachTimer = createTimer(nil, false)
timer_onTimer(autoAttachTimer, autoAttach)
timer_setInterval(autoAttachTimer, 5000)
timer_setEnabled(autoAttachTimer, true)

local versionsDef = {
    {"008245BC",18313216,0,"1.0","1.0"},
    {"0082457C",18313216,0,"1.0","1.0"},
    {"0082533C",34471936,0x2680,"1.01","1.01"},
    {"008252FC",34471936,0x2680,"1.01","1.01"},
    {nil,17985536,0x2680,"2.0","2.0"},
    {"0085EC4A",9691136,0x75130,"3.0 Steam","3.0 Steam"},
    {"0085DEDA",0,0x75770,"1.01 Steam","1.01 Steam?"},
    {nil,9981952,0x77970,"Steam","Steam (not fully supported)"}
}

local gameVersion = ""
local gameVersionDescription = ""

function detectVersion()
    gameVersion = ""
    gameVersionOffset = 0
    gameVersionAddr = {}

    -- Try to detect version by address
    -- (this doesn't work when CD hasn't been checked yet)
    local target = 38079
    for _,v in ipairs(versionsDef) do
        if v[1] ~= nil and readInteger(v[1]) == target then
            gameVersion = v[4]
            gameVersionDescription = v[5].." [detected by address]"
            gameVersionOffset = v[3] + getAddress(currentProcess)
        end
    end

    -- Detect by module size if no version detected yet
    if gameVersion == "" then
        local moduleSize = getModuleSize(currentProcess)
        for _,v in ipairs(versionsDef) do
            if v[2] ~= 0 and moduleSize == v[2] then
                gameVersion = v[4]
                gameVersionDescription = v[5].." [detected by module size]"
                gameVersionOffset = v[3] + getAddress(currentProcess)
            end
        end
    end

    -- Special Addresses
    if gameVersion == "Steam" then
        -- Steam Version needs more than an offset for some values
        local base = getAddress(currentProcess)
        gameVersionAddr[0x68B42C] = 0x702D98 + base
        gameVersionAddr[0x77CB84] = 0x80FD74 + base
        gameVersionAddr[0x773460] = 0x8002AC + base
    end

    if gameVersion == "" then
        gameVersionDescription = "Version Unknown"
    end
    GTASAForm.GameVersion.Caption = "Game Version: "..gameVersionDescription
end

