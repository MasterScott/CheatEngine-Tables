-----------------------------------
-- Game Version Address Translation
-----------------------------------

-- What to add to the relative 1.0 address to get the address for the current
-- version (e.g. also contains the process address, usually 0x400000).
local gameVersionOffset = 0

-- Contains special addresses, that require more than an offset to work.
-- The key is the normal 1.0 address, the value the absolute address.
local gameVersionAddr = {}

function adjustAddress(addr)
    if type(addr) == "string" then
        addr = tonumber(addr)
    end
    if addr == nil then
        return nil
    end
    if gameVersionAddr[addr] ~= nil then
        return gameVersionAddr[addr]
    end
    return addr+gameVersionOffset
end

function getInteger(addr)
    return readInteger(adjustAddress(addr))
end

function getByte(addr)
    return readBytes(adjustAddress(addr), 1, false)
end

function getFloat(addr)
    return readFloat(adjustAddress(addr))
end

